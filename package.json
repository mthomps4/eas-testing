{
  "name": "baby-groot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "doppler run -- expo start --dev-client --scheme babygroot-development",
    "android": "doppler run -- expo run:android --variant=DevelopmentDebug",
    "ios": "doppler run -- expo run:ios --scheme babygroot-development",
    "web": "doppler run -- expo start --web",
    "eas-build-pre-install": "chmod +x ./scripts/install-doppler.sh && ./scripts/install-doppler.sh",
    "asdf": "chmod +x ./scripts/doppler-run.sh && ./scripts/doppler-run.sh"
  },
  "dependencies": {
    "expo": "^45.0.0",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "^0.68.2",
    "react-native-web": "0.17.7",
    "uri-scheme": "^1.0.115"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "dotenv": "^16.0.0"
  },
  "private": true
}
